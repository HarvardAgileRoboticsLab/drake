<?xml version="1.0"?>
<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://drake.mit.edu ../../../../doc/drakeURDF.xsd" name="ballbot">
  <link name="cylinder">
    <visual>
      <geometry>
        <cylinder length="0.12" radius="0.03"/>
      </geometry>
      <origin xyz="0 0 0"/>
    </visual>
    <!-- add four collision point at the bottom of cylinder -->
    <collision>
      <origin xyz="0.03 0 -.06"/>
      <geometry>
        <sphere radius="0" />
      </geometry>
    </collision>
    <collision>
      <origin xyz="-0.03 0 -.06"/>
      <geometry>
        <sphere radius="0" />
      </geometry>
    </collision>
    <collision>
      <origin xyz="0 0.03 -.06"/>
      <geometry>
        <sphere radius="0" />
      </geometry>
    </collision>
    <collision>
      <origin xyz="0 -0.03 -.06"/>
      <geometry>
        <sphere radius="0" />
      </geometry>
    </collision>
    <collision>
      <geometry>
        <cylinder length="0.12" radius="0.03"/>
      </geometry>
      <origin xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.0014" ixy="0.0" ixz="0.0" iyy="0.0014" iyz="0.0" izz="0.00045"/>
      <!-- <inertia ixx="0.000525" ixy="0.0" ixz="0.0" iyy="0.000525" iyz="0.0" izz="0.00045"/> -->
      <origin xyz="0 0 0"/>
    </inertial>
  </link>
</robot>
