<?xml version="1.0"?>
<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://drake.mit.edu ../../doc/drakeURDF.xsd" name="FourBar">
  <link name="base_link">
    <inertial>
        <mass value="1"/>
        <inertia ixx="1"  ixy="0"  ixz="0" iyy="1" iyz="0" izz="1" />
        <origin xyz="0, 0, 0" rpy="0 0 0" /> 
    </inertial>
    <visual>
      <geometry>
        <cylinder length="3.6" radius="0.2"/>
      </geometry>
      <material>
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
  </link>

  <link name="link_1">
    <inertial>
        <origin xyz="0, 0, 0.9" rpy="0 0 0" />
        <mass value="1"/>
        <inertia ixx="1"  ixy="0"  ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0 0 .9" rpy="0 0 0"/>
      <geometry>
        <cylinder length="1.8" radius="0.2"/>
      </geometry>
      <material>
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
  </link>

  <link name="link_2">
    <inertial>
        <origin xyz="0, 0, 0.9" rpy="0 0 0" /> 
        <mass value="1"/>
        <inertia ixx="1"  ixy="0"  ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0 0 0.9" rpy="0 0 0"/>
      <geometry>
        <cylinder length="1.8" radius="0.2"/>
      </geometry>
      <material>
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
  </link>

  <link name="link_3">
    <inertial>
        <origin xyz="0, 0, 0.9" rpy="0 0 0" />
        <mass value="1"/>
        <inertia ixx="1"  ixy="0"  ixz="0" iyy="1" iyz="0" izz="1" />
    </inertial>
    <visual>
      <origin xyz="0 0 0.9" rpy="0 0 0"/>
      <geometry>
        <cylinder length="1.8" radius="0.2"/>
      </geometry>
      <material>
        <color rgba=".6353 .6353 .6353 1"/>
      </material>
    </visual>
    <!--<collision>
      <origin xyz="0 0 2.6"/>
      <geometry>
        <sphere radius="0" />
      </geometry>
    </collision> -->
  </link> 

