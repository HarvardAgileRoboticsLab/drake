    <origin rpy="0 0 0" xyz="0 0 1.8"/>
    <dynamics damping="1.3"/>
    <limit lower="pi/4" upper="5*pi/4" />
  </joint>-->

  <loop_joint name="joint_3" type="continuous">
    <axis xyz="0 1 0"/>
    <link1 link="link_3" rpy="0 0 0" xyz="0 0 1.8"/>
    <link2 link="base_link" rpy="0 0 0" xyz="0 0 0"/>
    <dynamics stiffness="0" damping="0"/>
  </loop_joint>

<force_element name="k0">
  <torsional_spring stiffness="5" rest_angle="0">
    <joint name="joint_0" />
  </torsional_spring>
 </force_element>

 <force_element name="k1">
  <torsional_spring stiffness="0" rest_angle="0">
    <joint name="joint_1" />
