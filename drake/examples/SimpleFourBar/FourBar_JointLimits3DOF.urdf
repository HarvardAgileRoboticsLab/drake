  <joint name="joint_0" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin rpy="0 0 0" xyz="0 0 1.8"/>
    <dynamics damping="1"/>
    <limit lower="pi/11" upper="10*pi/11" />
  </joint>

  <joint name="joint_1" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin rpy="0 0 0" xyz="0 0 1.8"/>
    <dynamics damping="1.1"/>
    <!--<limit lower="-pi" upper="pi" />-->
  </joint>

  <joint name="joint_2" type="continuous">
    <axis xyz="0 1 0"/>
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin rpy="0 0 0" xyz="0 0 1.8"/>
    <dynamics damping="1.2"/>
    <!--<limit lower="pi/4" upper="5*pi/4" />-->
  </joint>

  <!--<joint name="joint_3" type="continuous">
